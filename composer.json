{
  "name": "formaflow/backend-ddd",
  "type": "project",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "ramsey/uuid": "^4.7",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.10"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^11.0",
    "fakerphp/faker": "^1.24",
    "mockery/mockery": "^1.6",
    "squizlabs/php_codesniffer": "^4.0",
    "vimeo/psalm": "6.0.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "FormaFlow\\": "src/FormaFlow/",
      "Shared\\": "src/Shared/",
      "Database\\": "database/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
